{
    "name": "@j127/gatsby_website_starter",
    "version": "0.4.0",
    "description": "Everything you need for a basic website + blog with Gatsby 3",
    "main": "index.js",
    "scripts": {
        "format": "prettier \"**/*.{js,css,scss,html,jsx,json,md}\" --write",
        "develop": "gatsby develop",
        "start": "npm run develop",
        "build": "npm run clean && gatsby build",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "stage": "npm run build && netlify deploy --dir=public",
        "deploy": "npm run build && netlify deploy --prod --dir=public",
        "test": "echo 'write tests: https://www.gatsbyjs.com/docs/how-to/testing/'",
        "cy:open": "cypress open",
        "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/j127/gatsby_website_starter.git"
    },
    "keywords": [
        "gatsby",
        "web",
        "javascript",
        "gatsby-starter",
        "react"
    ],
    "author": "J. Cohen <hostelmarketing@gmail.com> (https://github.com/j127)",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/j127/gatsby_website_starter/issues"
    },
    "homepage": "https://github.com/j127/gatsby_website_starter#readme",
    "private": true,
    "dependencies": {
        "axios": "^0.21.1",
        "bulma": "^0.9.2",
        "gatsby": "^3.3.0",
        "gatsby-image": "^3.3.0",
        "gatsby-plugin-feed": "^3.3.0",
        "gatsby-plugin-google-gtag": "^3.3.0",
        "gatsby-plugin-manifest": "^3.3.0",
        "gatsby-plugin-no-sourcemaps": "^3.3.0",
        "gatsby-plugin-offline": "^4.3.0",
        "gatsby-plugin-purgecss": "^6.0.1",
        "gatsby-plugin-react-helmet": "^4.3.0",
        "gatsby-plugin-sass": "^4.3.0",
        "gatsby-plugin-sharp": "^3.3.0",
        "gatsby-remark-copy-linked-files": "^4.0.0",
        "gatsby-remark-images": "^5.0.0",
        "gatsby-remark-prismjs": "^5.0.0",
        "gatsby-remark-responsive-iframe": "^4.0.0",
        "gatsby-remark-smartypants": "^4.0.0",
        "gatsby-source-filesystem": "^3.3.0",
        "gatsby-transformer-remark": "^4.0.0",
        "gatsby-transformer-sharp": "^3.3.0",
        "prismjs": "^1.23.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "sass": "^1.32.8"
    },
    "devDependencies": {
        "babel-jest": "^26.6.3",
        "babel-preset-gatsby": "^1.1.0",
        "cypress": "^7.1.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "react-test-renderer": "^17.0.2",
        "start-server-and-test": "^1.12.1"
    }
}
